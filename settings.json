{
  "editor.renderControlCharacters": false,
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "editor.tabSize": 2,
  "editor.fontFamily": "Dank Mono",
  "editor.fontLigatures": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnSave": true,
  "editor.wordWrap": "off",
  "workbench.colorTheme": "Monokai Operator",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "window.activeBorder": "#d1cdcd11",
    "activityBar.activeBorder": "#FFA500",
    "activityBarBadge.background": "#ff0000",
    "editor.findMatchBackground": "#e29300c4",
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
  },
  "files.autoSave": "off",
  "window.zoomLevel": 1,
  "multiCommand.commands": [
    {
      "command": "multiCommand.toggleCommentAndLineDown",
      "sequence": [
        "editor.action.commentLine",
        "cursorDown"
      ]
    },
  ],
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.quoteStyle": "double",
  "javascript.preferences.quoteStyle": "double",
  "nativescript.analytics.enabled": false,
  "material-icon-theme.folders.color": "#90a4ae",
  "emmet.triggerExpansionOnTab": true,
  "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
  "bracket-pair-colorizer-2.showBracketsInGutter": true,
  "bracket-pair-colorizer-2.colors": [
    "#feda6a",
    "#FFA500",
    "#ec7f37",
    "#fa625f",
  ],
  "commentAnchors.workspace.excludeFiles": "**/{node_modules,platforms,.git,.idea,target,out,build,vendor}/**/*",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "vscode_custom_css.policy": true,
  "typescriptHero.imports.stringQuoteStyle": "\"",
  "vim.highlightedyank.enable": true,
  "vim.camelCaseMotion.enable": true,
  "vim.highlightedyank.duration": 500,
  "vim.statusBarColorControl": true,
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "#d1cdcd11",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "keyboard.dispatch": "keyCode",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "terminal.explorerKind": "external",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "prettier.printWidth": 100,
  "editor.renderWhitespace": "trailing",
  "editor.suggestSelection": "first",
  "terminal.integrated.rendererType": "dom",
}